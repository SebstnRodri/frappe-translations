"\toptions: \"Empresa\",\n"
"\tdefault: frappe.defaults.get_user_default(\"Empresa\"),\n"
"\treqd: 1\n"
"},\n"
"{\n"
"\tfieldname: \"account\",\n"
"\tlabel: __(\"Conta\"),\n"
"\tfieldtype: \"Link\",\n"
"\toptions: \"Conta\",\n"
"\treqd: 1\n"
